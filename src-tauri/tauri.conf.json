{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "proedgeboost",
  "version": "0.1.0",
  "identifier": "com.proedgeboost.app",
  "build": {
    "beforeDevCommand": "yarn dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "yarn build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "慕讯公益加速器",
        "decorations": false,
        "transparent": true,
        "resizable": false,
        "width": 1,
        "height": 1,
        "visible": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
